<comment>EMPLOYEE SQL</comment>
<entry key="A">B</entry>
<entry key="findAll">
	SELECT 
		   EMP_ID
	     , EMP_NAME
         , SALARY
	     , DEPT_TITLE
	     , JOB_NAME
  	  FROM
  	 	   EMPLOYEE
  	  JOIN 
   	 	   JOB USING (JOB_CODE)
      LEFT
      JOIN
	       DEPARTMENT ON (DEPT_CODE = DEPT_ID)
	 ORDER 
 		BY 
   		   EMP_ID ASC
</entry>
